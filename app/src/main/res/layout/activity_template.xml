<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/how_select_upload_background">


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/template_margin_left"
        >

        <TextView
            style="@style/tv_title"
            android:layout_marginTop="@dimen/distance_small"
            android:text="@string/template_add_title"
            android:textColor="@color/textWhite"
            android:textSize="@dimen/font_title"
            android:textStyle="bold"/>

        <TextView
            style="@style/tv_title"
            android:layout_marginBottom="57dp"
            android:layout_marginTop="21dp"
            android:text="@string/template_add_placeholer"
            android:textColor="@color/textWhite"
            android:textSize="@dimen/template_add_placeholer"
            />

        <com.prettyyes.user.app.ui.appview.TemplateItemView
            android:id="@+id/tempItem_brand"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="brand"
            app:template_desc="@string/template_add_brand_desc"
            app:template_icon="@mipmap/how_brand_upload_icon"
            app:template_range="@string/template_add_brand_range"
            app:template_type="@string/template_add_brand"/>

        <com.prettyyes.user.app.ui.appview.TemplateItemView
            android:id="@+id/tempItem_unit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="unit"/>

        <com.prettyyes.user.app.ui.appview.TemplateItemView
            android:id="@+id/tempItem_taobao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="taobao"
            app:template_desc="@string/template_add_taobao_desc"
            app:template_icon="@mipmap/how_speed_upload_icon"
            app:template_range="@string/template_add_taobao_range"
            app:template_type="@string/template_add_taobao"/>

        <com.prettyyes.user.app.ui.appview.TemplateItemView
            android:id="@+id/tempItem_paper"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="paper"
            app:template_desc="@string/template_add_paper_desc"
            app:template_icon="@mipmap/how_content_upload_icon"
            app:template_range="@string/template_add_paper_range"
            app:template_type="@string/template_add_paper"/>

        <com.prettyyes.user.app.ui.appview.TemplateItemView
            android:id="@+id/tempItem_suit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="suit"
            app:template_bottom_line_show="false"
            app:template_desc="@string/template_add_suit_desc"
            app:template_icon="@mipmap/how_multiple_uplaod_icon"
            app:template_range="@string/template_add_suit_range"
            app:template_type="@string/template_add_suit"/>
    </LinearLayout>

